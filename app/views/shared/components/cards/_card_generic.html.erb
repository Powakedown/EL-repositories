<article class="card-generic <%= "card-generic-opened" if resource == 1 %>">
  <div class="card-generic-top">
    <h3><%= resource.name %></h3>
    <div class="card-generic-stars">
      <%= resource.stargazers_count %> <%= image_tag "pictos/star-solid.svg", class:"#{"active" if resource.starred? }" %>
    </div>
  </div>
  <div class="card-generic-details" id="card-generic-details-<%= resource.id %>">
  </div>
</article>
